<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="RemoteImageView">
    <attr name="url" format="string" />
    <attr name="cached_width" format="integer" />
    <attr name="cached_height" format="integer" />

    <!-- ImageView attributes -->
    <attr name="adjustViewBounds" format="boolean" />
    <attr name="maxHeight" format="integer" />
    <attr name="maxWidth" format="integer" />
    <attr name="scaleType" format="enum">
      <enum name="matrix" value="0" />
      <enum name="fixXY" value="1" />
      <enum name="fitStart" value="2" />
      <enum name="fitCenter" value="3" />
      <enum name="fitEnd" value="4" />
      <enum name="center" value="5" />
      <enum name="centerCrop" value="6" />
      <enum name="centerInside" value="7" />
    </attr>
    <attr name="src" format="reference" />

  </declare-styleable>
</resources>
